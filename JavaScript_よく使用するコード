//宣言
function 関数名(引数1,引数2,…) {
　　処理
　return 返り値;
}

var //よくある宣言(バグ多し)
let //ブロックスコープのローカル変数を宣言
const //ブロックスコープで読み取り専用の名前付き定数を宣言

//繰り返し文
for(let i = 1; i <= A.length -1;i++){
    処理
｝

//条件式
if(a[j] <= x){
　　処理
}

//出力
console.log(); //指定データを表示
console.log({変数,変数}); //変数を表示してデータを複数表示

console.table([ foo, bar, baz ]); //テーブル表示

console.time(); //時間計測
console.log("タイマー開始");
console.timeEnd()；
console.log("タイマー終了");
