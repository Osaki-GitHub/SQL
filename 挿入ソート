function main(input){
	let array = input.split("\n");
	let N array[0];
	let A array[1].split(" ");
	console.log(array[1]);
	for(let i = 1; i <= A.length-1; i++){
		let v = A[i];
		let j = i - 1;
		while(j >= 0; && A[j] > v){
			A[j+1] = A[j];
			j--;
		}
		A[j+1] = v;
    console.log(A.join(" "))
  }
}

main(require('fs').readFileSync('/dev/stdin', 'UTF-8'));
